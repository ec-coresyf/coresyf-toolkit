            "type": "int",
            "default": 0
        },
        {
            "identifier": "PsourceBands",
            "name": "sourceBands",
            "description": "The list of source bands",
            "type": "string" 
        },
        {
            "identifier": "PuseSRTM",
            "name": "useSRTM",
            "description": "Set useSRTM to <boolean>",
            "type": "boolean",
            "default": true
        }
        ],
        "outputs": [
          {
            "identifier": "Ttarget",
            "name": "target",
