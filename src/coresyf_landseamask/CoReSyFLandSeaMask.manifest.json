        {
            "identifier": "Ssource",
            "name": "source",
            "description": "Sets source to <filepath>",
            "type": "Raster"
        }
     ],
     "parameters": [
        {
            "identifier": "Pgeometry",
            "name": "geometry",
            "description": "Sets geometry to <string>",
            "type": "string"
        },
        {
            "identifier": "PinvertGeometry",
            "name": "invertGeometry",
            "description": "Sets invertGeometry to <boolean>",
